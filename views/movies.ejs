<%- include('partials/header') %>
<h1>Movies</h1>
<ul>
    <% movies.forEach(movie => { %>
        <li>
            <a href="/movies/<%= movie.id %>">
                <img src="<%= movie.image %>" alt="<%= movie.title %>">
                <h2><%= movie.title %> (<%= movie.year %>)</h2>
            </a>
        </li>
    <% }) %>
</ul>
<%- include('partials/footer') %>
