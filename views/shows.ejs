<%- include('partials/header') %>
<h1>TV Shows</h1>
<ul>
    <% shows.forEach(show => { %>
        <li>
            <a href="/shows/<%= show.id %>">
                <img src="<%= show.image %>" alt="<%= show.title %>">
                <h2><%= show.title %> (<%= show.year %>)</h2>
            </a>
        </li>
    <% }) %>
</ul>
<%- include('partials/footer') %>
